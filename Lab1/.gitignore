# .gitignore for C# project with JetBrains Rider

# Ignore Rider-specific files
.idea/
*.sln.iml
*.csproj.iml
*.DotSettings
*.DotSettings.user

# Ignore build and output directories
[Bb]in/
[Oo]bj/
[Tt]est[Rr]esult*/
[Bb]uild*/
[Oo]utput/

# Ignore NuGet packages and restore files
[Nn]u[Gg]et/
*.nupkg
packages/
nuget.exe

# Ignore local user-specific files
*.user
*.suo
*.userprefs
*.lock.json

# Ignore ReSharper files
_ReSharper.*
*.ReSharper

# Ignore Unity files (if applicable)
[Ll]ibrary/
[Tt]emp/
[Oo]bj/
[Ll]ogs/
[Uu]ser[Ss]ettings/

# Ignore VS Code files (if applicable)
.vscode/

# Ignore macOS files
.DS_Store

# Ignore local development files
.env
.local

# Ignore logs and diagnostics
[Ll]og/
[Ll]ogs/
*.log
*.tlog

# Ignore Azure Functions local settings
local.settings.json

# Ignore test results
[Tt]est[Rr]esults/
*.trx

# Ignore coverage reports
[Cc]overage/
*.coverage
*.coveragexml

# Ignore Fody weavers
FodyWeavers.xsd
FodyWeavers.xml

# Ignore MSTest files
*.TestSettings
*.testlog

# Ignore dotnet tools
.dotnet/
tools/

# Ignore Cake build files
tools/
!.cake/

# Ignore JetBrains Rider cache
.idea_modules/